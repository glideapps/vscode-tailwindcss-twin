{
  "extends": "../tsconfig",
  "compilerOptions": {
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "typeRoots": ["./typings"],
    "lib": ["esnext", "dom", "dom.iterable", "webworker"],
    "paths": {
      "~/*": ["./*"],
      "assets/*": ["./assets/*"]
    }
  },
  "include": ["./**/*"]
}
